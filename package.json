{
	"name": "print",
	"displayName": "Print via browser",
	"description": "",
	"version": "0.0.1",
	"author": {
		"name": "Peter Wone",
		"email": "peter.wone@outlook.com"
	},
	"publisher": "pdconsec",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.print"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.print",
				"title": "Print"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId",
					"command": "extension.print",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"when": "resourceLangId",
					"command": "extension.print",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.26",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"@types/highlight.js": "^9.12.3",
		"@types/tmp": "0.0.34",
		"highlight.js": "^9.14.2",
		"tmp": "0.0.33"
	}
}
